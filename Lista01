//(FLOAT) some 10.5 e exiba.
#include <stdio.h>
int main(){
  float X;

  printf("INSIRA:\n");
  scanf("%f",&X);

  printf("RESULTADO:%.2f",X + 10.5);
  
  return 0;
}

//(INT) 4 variáveis e média.
#include <stdio.h>
int main(){
   int X,Y,Z,A;
   int Media;

   printf("INSIRA 4(QUATRO) VALORES:\n");
   scanf("%d %d %d %d",&X,&Y,&Z,&A);

   Media = (X+Y+Z+A)/4;

   printf("MÉDIA:%d\n",Media);
  
  return 0;
}

//PEDIR DOIS NÚMEROS E IMPRIMIR DIFERENÇA  .
#include <stdio.h>
int main(){
   int X,Y;

   printf("INSIRA 2 VALORES:\n");
   scanf("%d %d",&X,&Y);

   printf("DIFERENÇA:%d\n",X/Y);
  
  return 0;
}
