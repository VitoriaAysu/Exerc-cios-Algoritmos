//Atribua valor a duas variáveis (INT) e imprima a soma.
#include <stdio.h>
int main(){
  int X;
  int Y;

  printf("Insira dois valores:\n");
  scanf("%d %d",&X,&Y);

  printf("SOMA:%d\n",X+Y);

  return 0;
}

//Atribua valor a duas variáveis (FLOAT) e imprima a subtração.
#include <stdio.h>
int main(){
  float X;
  float Y;

  printf("Insira dois valores:\n");
  scanf("%f %f",&X,&Y);

  printf("SUBTRAÇÃO:%.2f\n",X-Y);

  return 0;
}

//MULTIPLICAÇÃO DOIS NUMEROS INTEIROS E IMPRESSÃO RESULTADO.
#include <stdio.h>
int main(){
  int X;
  int Y;

  printf("Insira dois valores:\n");
  scanf("%d %d",&X,&Y);

  printf("MULTIPLICAÇÃO:%d\n",X*Y);

  return 0;
}

